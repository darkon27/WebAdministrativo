    <script>

        function convertirAMayusculas(input) {
            input.value = input.value.toUpperCase();
        }

        function GoPageSelect(IdGrupo, IdMenu, Href) {
            var perpObj = {
                IdGrupoRequest: IdGrupo,
                IdMenuRequest: IdMenu,
                HrefRequest: Href

            };
            $.ajax({

                url: "/MainAdmin/reqMenu",
                data: JSON.stringify(perpObj),
                type: "POST",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    window.location.href = result;

                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }



        function CerrarSesion() {

            swal({
                title: "¿Desea Cerrar Sesión?",
                text: "Se direccionará a la pantalla de autentificación..!!",
                type: "warning",
                showCancelButton: true,
                confirmButtonClass: "btn-danger",
                confirmButtonText: "Aceptar",
                closeOnConfirm: false
            },
                function () {
                    DeadSession();

                });
        }

        function DeadSession() {
            $.ajax({
                url: "/LoginAdmin/CerrarSesion",
                data: "CERRAR",
                type: "POST",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (result == 9) {
                        history.go(1);
                        window.history.forward(1);
                        window.onbeforeunload = function () {
                            window.history.forward(1);
                        };

                        window.location.href = '/LoginAdmin/Login';
                        location.reload();
                    }
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }
    </script>

